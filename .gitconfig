[user]
	name        = veirus
	email       = thetauleherves@gmail.com
[alias]
	co          = checkout
	ci          = commit
	st          = status
	br          = branch
	hist        = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	lg          = log --all --decorate --graph --date=short --format=format:'%Cblue%h%Creset %Cgreen%ad (%ar)%C(bold yellow)%d%Creset%n        %C(white)%s%Creset%n        %an'
	type        = cat-file -t
	dump        = cat-file -p
[oh-my-zsh]
	hide-status = 1
	hide-dirty  = 1
[core]
	autocrlf    = input
