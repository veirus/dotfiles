version 6.0
if &cp | set nocp | endif
let s:cpo_save=&cpo
set cpo&vim
map! <S-Insert> *
vmap  "*d
vmap <C-Del> "*d
vmap <S-Del> "*d
vmap <C-Insert> "*y
vmap <S-Insert> "-d"*P
nmap <S-Insert> "*P
let &cpo=s:cpo_save
unlet s:cpo_save
set guifont=Consolas:h12
set guioptions=egmrLt
set helplang=ru
set laststatus=2
set noloadplugins
set ruler
set wildmenu
" vim: set ft=vim :
